													      <input type="text" value="$!{appmodel.newacturl}" name="newacturl" id="newacturl" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">模块是否启用</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{appmodel.active}" name="active" id="active" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">模块名称</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{appmodel.appname}" name="appname" id="appname" class="form-control" />
