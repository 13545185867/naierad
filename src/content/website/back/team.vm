$(".match-item").click(function(){
    $(".match-item-active").prop('class','match-item');
    $(this).prop('class','match-item-active');
});

$(".turn-right").click(function(){
    var list = $(".match-list");
	 console.log(list.scrollWidth);
	console.log(list.scrollLeft());
    if(list.scrollWidth - list.scrollLeft() < 989){
        list.animate({scrollLeft: list.scrollWidth},989);
    }else{
        list.animate({scrollLeft:list.scrollLeft()+989},1000);
    }
});

$(".turn-left").click(function(){
    var list = $(".match-list");
		 console.log(list.scrollWidth);
	console.log(list.scrollLeft());
    if(list.scrollLeft() < 989){
        list.animate({scrollLeft:0},1000);
    }else{
        list.animate({scrollLeft:list.scrollLeft()-989},1000);
    }
});
</script>
